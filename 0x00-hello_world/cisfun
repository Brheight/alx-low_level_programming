#!/bin/bash

# Set the C file name
CFILE=$1

# Check if the C file name is provided
if [ -z "$CFILE" ]; then
    echo "Please provide the C file name."
    exit 1
fi

# Check if the C file exists
if [ ! -f "$CFILE" ]; then
    echo "File not found: $CFILE"
    exit 1
fi

# Compile the C file and create the executable
gcc "$CFILE" -o cisfun
